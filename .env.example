# Clerk public key for client-side authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Clerk secret key for server-side authentication
CLERK_SECRET_KEY=

# Clerk authentication routes
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in      # Sign-in page
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up      # Sign-up page

# Redirect URLs after login or signup
NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL=/   # Redirect after login
NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL=/   # Redirect after signup

# Clerk webhook secret for verifying incoming webhooks
CLERK_WEBHOOK_SIGNING_SECRET=

# Database connection string (e.g., PostgreSQL, MySQL, etc.)
DATABASE_URL=

# Upstash Redis credentials for caching, sessions, or rate limiting
UPSTASH_REDIS_REST_URL=""       # Upstash Redis REST API URL
UPSTASH_REDIS_REST_TOKEN=""     # Upstash Redis authentication token

# Mux credentials for video uploads and streaming
MUX_TOKEN_ID=                   # Mux token ID
MUX_TOKEN_SECRET=               # Mux token secret
MUX_WEBHOOK_SECRET=             # Mux webhook signing secret

# UploadThing token for file uploads
UPLOADTHING_TOKEN=

# QStash & Upstash Workflows credentials for background jobs or webhooks
QSTASH_TOKEN=                   # QStash API token
UPSTASH_WORKFLOW_URL=           # Upstash workflow endpoint
QSTASH_CURRENT_SIGNING_KEY=     # Current QStash signing key
QSTASH_NEXT_SIGNING_KEY=        # Next QStash signing key (for key rotation)

# Gemini API key for AI integrations
GEMINI_API_KEY=

# Base URL of your app (used in redirects, webhooks, etc.)
NEXT_PUBLIC_APP_URL=
